<Window x:Class="S3_Deadline.Login"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:S3_Deadline"
        mc:Ignorable="d"
        Title="Login" Height="200" Width="250">
    <Grid>
        <Label Content="Brugernavn"
               Margin="0,23,180,136"></Label>
        <TextBox Width="100" Height="25"
                 x:Name="textBoxUserName"
                 Margin="125,25,25,134"></TextBox>
        <Label Content="Adgangskode"
               Margin="-1,80,161,71"></Label>
        <TextBox Width="100"
                 Height="25"
                 x:Name="textBoxPassword"
                 Margin="124,78,26,81"></TextBox>
        <Button Margin="81,150,81,2" Content="Log ind"
                x:Name="buttonLogin"
                Click="ButtonLogin_Click"></Button>
    </Grid>
</Window>
